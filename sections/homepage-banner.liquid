{% schema %}
  {
    "name": "Homepage banner",
    "settings": [
      {
        "type": "richtext",
        "id": "title",
        "default": "<p>Title</p>",
        "label": "t:sections.footer.blocks.text.settings.heading.label"
      },
	  {
        "type": "richtext",
        "id": "subtitle",
        "default": "<p>Description</p>",
        "label": "t:sections.footer.blocks.text.settings.subtext.label"
      },
	  {
        "type": "image_picker",
        "id": "image",
        "label": "Image"
      }
	],
	"presets": [
    {
      "name": "Homepage banner"
    }
  	]
  }
{% endschema %}

